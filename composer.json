{
  "name": "mati365/ckeditor5-symfony",
  "version": "1.2.2",
  "description": "CKEditor 5 integration for Symfony",
  "type": "symfony-bundle",
  "license": "MIT",
  "keywords": [
    "symfony",
    "ckeditor",
    "ckeditor5",
    "wysiwyg",
    "editor",
    "rich-text-editor",
    "text-editor",
    "html-editor",
    "twig",
    "content-editor"
  ],
  "authors": [
    {
      "name": "Mateusz Baginski",
      "email": "cziken58@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.3",
    "respect/validation": "^2.4|^3.0",
    "twig/twig": "^3.0",
    "symfony/asset": "^6.4|^7.0|^8.0",
    "symfony/console": "^6.4|^7.0|^8.0",
    "symfony/http-foundation": "^6.4|^7.0|^8.0",
    "symfony/http-kernel": "^6.4|^7.0|^8.0",
    "symfony/http-client": "^6.4|^7.0|^8.0",
    "symfony/form": "^6.4|^7.0|^8.0",
    "symfony/dependency-injection": "^6.4|^7.0|^8.0"
  },
  "require-dev": {
    "psalm/plugin-symfony": "^5.2.8",
    "vimeo/psalm": "^6.14.3",
    "friendsofphp/php-cs-fixer": "^3.88",
    "phpunit/phpunit": "^11.0",
    "mikey179/vfsstream": "^1.6"
  },
  "scripts": {
    "test": "phpunit",
    "test:coverage": "env XDEBUG_MODE=coverage php -d pcov.enabled=1 -d pcov.directory=. ./vendor/bin/phpunit --coverage-html coverage --coverage-text"
  },
  "autoload": {
    "psr-4": {
      "Mati365\\CKEditor5Symfony\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mati365\\CKEditor5Symfony\\Tests\\": "tests/"
    }
  }
}
