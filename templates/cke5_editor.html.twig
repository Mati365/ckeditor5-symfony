<cke5-editor
    {% if class is not null %}class="{{ class }}"{% endif %}
    {% if style is not null %}style="{{ style }}"{% endif %}
    data-cke-editor-id="{{ id }}"
    data-cke-preset="{{ preset|e('html_attr') }}"
    data-cke-content="{{ content|e('html_attr') }}"
    data-cke-language="{{ language|e('html_attr') }}"
    data-cke-save-debounce-ms="{{ saveDebounceMs }}"
    {% if watchdog %}data-cke-watchdog="true"{% endif %}
    {% if contextId is not null %}data-cke-context-id="{{ contextId }}"{% endif %}
    {% if editableHeight is not null %}data-cke-editable-height="{{ editableHeight }}"{% endif %}
    data-turbo-temporary
>
    {% if show_input %}
        <div id="{{ id }}_editor"></div>
    {% endif %}

    {% if name is not null %}
        {{ cke5_hidden_input(name: name, id: id ~ '_input', required: required) }}
    {% endif %}
</cke5-editor>
